Config { font = "-*-terminus-medium-*-*-*-12-*-*-*-*-*-*-*"
       , bgColor = "#101010"
       , fgColor = "#757575"
       , lowerOnStart = True
       , commands = [ Run Weather "KLUK" ["-t"," <tempF>F"] 36000
                    , Run Date "%a %b %d %Y-%m-%d :: %H:%M:%S" "date" 10
                    , Run StdinReader
                    ]
       , sepChar = "%"
       , alignSep = "}{"
       , template = " %StdinReader% }{ %date% ::%KLUK% "
       }
