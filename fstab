# /etc/fstab
# Andrew Antle - http://antlechrist.org/
# Static file system information.

# <fs>      <dir>        <type>  <options>        <dump>  <pass> 
devpts      /dev/pts     devpts  gid=5,mode=620   0       0
proc        /proc        proc    defaults         0       0
tmpfs       /dev/shm     tmpfs   defaults         0       0

/dev/cdrom  /mnt/cdrom   auto    noauto,owner,ro  0       0
/dev/fd0    /mnt/floppy  auto    noauto,owner     0       0

/dev/sdb2   swap         swap    defaults         0       0
/dev/sdb1   /            ext4    defaults         1       1
/dev/sdb3   /home        ext4    defaults         1       2

/dev/sdb4   /mnt/sdb4    ext4    defaults         1       2

/dev/sdc1   /mnt/sdc1    ext3    noauto,user      0       0
/dev/sdc2   /mnt/sdc2    ext3    noauto,user      0       0
/dev/sdc3   /mnt/sdc3    ext3    noauto,user      0       0
/dev/sdc4   /mnt/sdc4    ext3    noauto,user      0       0

/dev/sdd    /mnt/tmp     vfat    noauto,user      0       0
